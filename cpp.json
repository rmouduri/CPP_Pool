{
	"Canonical Class": {
		"prefix": "cclass",
		"body": [
			"# include <iostream>",
			"",
			"class $1",
			"{",
			"private:",
			"public:",
			"\t$1();",
			"\t$1(const $1& new$1);",
			"\t~$1();",
			"",
			"\t$1& operator=(const $1& new$1);",
			"};",
			"",
			"$1::$1(void)",
			"{",
			"\tstd::cout << \"Default Constructor for $1 called\" << std::endl;",
			"\treturn ;",
			"}",
			"",
			"$1::$1(const $1& new$1)",
			"{",
			"\tstd::cout << \"Copy Constructor for $1 called\" << std::endl;",
			"\treturn ;",
			"}",
			"",
			"$1::~$1(void)",
			"{",
			"\tstd::cout << \"Default Destructor for $1 called\" << std::endl;",
			"\treturn ;",
			"}",
			"",
			"$1& $1::operator=(const $1& new$1)",
			"{",
			"\tstd::cout << \"$1 Assignement Operator called\" << std::endl;",
			"\treturn *this;",
			"}"
		],
		"description": "Canonical class for c++"
	}
}
